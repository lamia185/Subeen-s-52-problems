#include<bits/stdc++.h>
using namespace std;

int main()
{

    int t;
    cin>>t;

    while(t--){

        int n;
        cin>>n;

        for(int i=1;i<=n*n;i++){ //We dont need extra another loop 
            
              cout<<"*";
              if(i%n==0)
                  cout<<endl;

        }

        cout<<endl;

    }

}
